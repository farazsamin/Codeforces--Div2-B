/*
Link      :  https://codeforces.com/problemset/problem/1391/B


Approach  :


 
*/
#include<bits/stdc++.h>
using namespace std;


int main(){
    long long t,n,m;
    char ch;
    cin>>t;
    while(t--){
    	cin>>n>> m;
	    int ans=0;
	    for(int i = 1;i <= n;i++){
		    for(int j = 1;j <= m;j++){
			    cin >>ch;
				if(ch == 'C') continue;
				if(i == n and ch == 'D')ans++;
				if(j == m and ch == 'R')ans++;
		}
	}
	cout <<ans<< endl;
    }

}
